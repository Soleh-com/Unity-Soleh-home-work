using System;
					
public class Program
{
	int number = 1 ;
	
	public  void Main()
	{
		Console.WriteLine("Пожалуйста введите своё имя  ");
		string name = Console.ReadLine();
		Console.Write("Ваше имя "+name);
		Console.WriteLine("Пожалуйста введите свой возраст  ");
		number = Int32.Parse( Console.ReadLine());
		Console.Write("Ваш возраст "+number);
		Console.WriteLine("Пожалуйста введите свой хобби  ");
		string names = Console.ReadLine();
		Console.Write("Ваш хобби "+names);
		Console.WriteLine("Пожалуйста введите свой пол  ");
		string namess = Console.ReadLine();
		Console.Write("Ваш пол "+namess);
		
	}
}
